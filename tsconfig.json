{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "paths": {
      "*": ["./src/*"],
      "components/*": ["./src/components/*"],
      "locales/*": ["./src/locales/*"],
      "models/*": ["./src/models/*"],
      "navigations/*": ["./src/navigations/*"],
      "reduxs/*": ["./src/reduxs/*"],
      "screens/*": ["./src/screens/*"],
      "services/*": ["./src/services/*"],
      "types/*": ["./src/types/*"],
      "utils/*": ["./src/utils/*"]
    }
  }
}
